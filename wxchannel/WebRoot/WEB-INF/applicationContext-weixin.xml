<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xmlns:jdbc="http://www.springframework.org/schema/jdbc" xmlns:jee="http://www.springframework.org/schema/jee"
	xmlns:tx="http://www.springframework.org/schema/tx" xmlns:jpa="http://www.springframework.org/schema/data/jpa"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.1.xsd
		http://www.springframework.org/schema/jdbc http://www.springframework.org/schema/jdbc/spring-jdbc-3.1.xsd
		http://www.springframework.org/schema/jee http://www.springframework.org/schema/jee/spring-jee-3.1.xsd
		http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-3.1.xsd
		http://www.springframework.org/schema/data/jpa http://www.springframework.org/schema/data/jpa/spring-jpa-1.1.xsd">

	<bean name="wxchannelsysConf" class="com.rockstar.o2o.weixin.conf.SysConf">
		<property name="baseAccessTokenUrl" value="https://api.weixin.qq.com/cgi-bin/token?" />
		<property name="baseUserinfoUrl" value="https://api.weixin.qq.com/cgi-bin/user/info?" />
		<property name="baseSendMsgUrl"
			value="https://api.weixin.qq.com/cgi-bin/message/send?" />
		<property name="baseCustomSendMsgUrl"
			value="https://api.weixin.qq.com/cgi-bin/message/custom/send?" />
		<property name="baseQrcodeUrl"
			value="https://api.weixin.qq.com/cgi-bin/qrcode/create?" />
		<property name="baseQrcodePicUrl"
			value="https://mp.weixin.qq.com/cgi-bin/showqrcode?ticket=" />
		<property name="baseCreateMenuUrl"
			value="https://api.weixin.qq.com/cgi-bin/menu/create?" />
		<property name="baseQueryMenuUrl" value="https://api.weixin.qq.com/cgi-bin/menu/get?" />
		<property name="baseDeleteMenuUrl"
			value="https://api.weixin.qq.com/cgi-bin/menu/delete?" />
		<property name="baseSubscribeUserListUrl" value="https://api.weixin.qq.com/cgi-bin/user/get?" />
		<property name="baseOAuthUrl"
			value="https://open.weixin.qq.com/connect/oauth2/authorize?" />
		<property name="baseOAuthTokenUrl"
			value="https://api.weixin.qq.com/sns/oauth2/access_token?" />
		<property name="baseLocalOAuthRedirectUrl"
			value="http://wx.chorhair.com/oauth/redirect.php" />
		<property name="baseDeliverNotifyUrl"
			value="https://api.weixin.qq.com/pay/delivernotify?" />
		<property name="baseShortUrl"
			value="https://api.weixin.qq.com/cgi-bin/shorturl?" />
		<property name="baseUploadMediaUrl"
			value="http://file.api.weixin.qq.com/cgi-bin/media/upload?" />			
		<property name="baseUploadNewsUrl"
			value="https://api.weixin.qq.com/cgi-bin/media/uploadnews?" />
		<property name="baseSendNewsBylistUrl"
			value="https://api.weixin.qq.com/cgi-bin/message/mass/send?" />	
		 <property name="baseDownloadMediaUrl"
			value="http://file.api.weixin.qq.com/cgi-bin/media/get?" />			
	</bean>
</beans>
